        <Button layoutX="282.0" layoutY="167.0" mnemonicParsing="false" onAction="#handleChooseFile" text="Choose" />
        <Label layoutX="20.0" layoutY="31.0" text="Title" />
        <Label layoutX="17.0" layoutY="77.0" text="Artist" />
        <Label layoutX="8.0" layoutY="123.0" text="Category" />
        <Label layoutX="22.0" layoutY="171.0" text="File" />
        <TextField fx:id="songTitleField" layoutX="67.0" layoutY="27.0" prefHeight="26.0" prefWidth="198.0" />
